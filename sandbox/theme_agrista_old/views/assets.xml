<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <template id="replace_bootstrap" inherit_id="website.theme" name="Agrista Theme">
            <xpath expr="//link[@id='bootstrap_css']" position="replace">
                <link rel="stylesheet" type="text/css" href="/theme_agrista/static/src/css/website.css"/>
                <link rel="stylesheet" type="text/css" href="/theme_agrista/static/lib/agricons/css/agricons.css"/>
            </xpath>
        </template>
        <template id="web" inherit_id="web.assets_backend" name="Base Agrista Theme Assets">
            <xpath expr="//link[last()]" position="after">
                <link rel="stylesheet" type="text/css" href="/theme_agrista/static/src/css/web.css"/>
            </xpath>
        </template>
        <template id="website_resets" name="Odoo Bootstrap resets" inherit_id="website.theme">
            <xpath expr="link[last()]" position="after">
                <link href="/theme_agrista/static/src/css/resets.css" rel="stylesheet" type="text/css"/>
            </xpath>
        </template>
    </data>
</openerp>